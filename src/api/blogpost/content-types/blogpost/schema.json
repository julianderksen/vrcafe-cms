{
  "kind": "collectionType",
  "collectionName": "blogposts",
  "info": {
    "singularName": "blogpost",
    "pluralName": "blogposts",
    "displayName": "Blogpost"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "date": {
      "type": "datetime"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "blogposts"
    },
    "tags": {
      "type": "json"
    },
    "imageUrl": {
      "type": "string"
    },
    "featured": {
      "type": "boolean"
    },
    "readTime": {
      "type": "integer"
    },
    "content": {
      "type": "blocks"
    }
  }
}
